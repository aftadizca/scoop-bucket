{
    "homepage": "https://github.com/NetchX/Netch",
    "description": "Game accelerator. Support Socks5, Shadowsocks, ShadowsocksR, V2Ray protocol. UDP NAT FullCone",
    "license": "MIT",
    "version": "1.4.4",
    "architecture": {
        "64bit": {
            "url": "https://github.com/NetchX/Netch/releases/download/1.4.4/Netch-1.4.4.zip",
            "hash": "cf148341ce98ade8539eba8c2bde439d34f79f494c4654aa3b1645e6f13ac13e"
        }
    },
    "_architecture": {
        "32bit": {
            "url": "https://github.com/NetchX/Netch/releases/download/1.4.3/Netch.x86-1.4.3.zip",
            "hash": "19af1c3e216c726566564b8000afaa2f4a67170462c1c51a92bd907eafb7d7d6"
        }
    },
    "pre_install": [
        "foreach ($_ in Get-ChildItem -File \"$dir\\mode\") {",
        "    if (!(Test-Path \"$persist_dir\\mode\\$($_.Name)\")) {",
        "        Copy-Item \"$($_.FullName)\" \"$persist_dir\\mode\" | Out-Null",
        "    }",
        "}",
        "Rename-Item -Path \"$dir\\mode\" -NewName \"$dir\\mode.original\""
    ],
    "persist": [
        "data",
        "mode"
    ],
    "shortcuts": [
        [
            "Netch.exe",
            "Netch"
        ]
    ],
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/NetchX/Netch/releases/download/$version/Netch-$version.zip"
            }
        },
        "hash": {
            "url": "https://github.com/NetchX/Netch/releases/tag/$version",
            "regex": "<td align=\"left\"><sub>$basename</sub></td>[\\r\\n]+<td align=\"left\"><sub>$sha256</sub></td>"
        }
    }
}
