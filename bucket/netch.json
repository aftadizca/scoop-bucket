        }
    },
    "pre_install": [
        "foreach ($_ in Get-ChildItem -File \"$dir\\mode\") {",
        "    if (!(Test-Path \"$persist_dir\\mode\\$($_.Name)\")) {",
        "        Copy-Item \"$($_.FullName)\" \"$persist_dir\\mode\" | Out-Null",
        "    }",
        "}"
    ],
    "persist": [
        "data",
        "mode"
    ],
    "shortcuts": [
        [
            "Netch.exe",
            "Netch"
        ]
    ],
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/NetchX/Netch/releases/download/v$version/Netch.x64-v$version.zip",
                "hash": {
                    "url": "https://github.com/NetchX/Netch/releases/tag/v$version",
                    "regex": "<td align=\"left\"><sub>$basename</sub></td>[\\r\\n]+<td align=\"left\"><sub>$sha256</sub></td>"
                }
            },
            "32bit": {
                "url": "https://github.com/NetchX/Netch/releases/download/v$version/Netch.x86-v$version.zip",
                "hash": {
                    "url": "https://github.com/NetchX/Netch/releases/tag/v$version",
                    "regex": "<td align=\"left\"><sub>$basename</sub></td>[\\r\\n]+<td align=\"left\"><sub>$sha256</sub></td>"
                }
            }
        }
    }
}
